{
  "private": true,
  "name": "codsen-com",
  "version": "2.0.0",
  "description": "codsen.com website",
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn run build:css && remix build",
    "build:css": "postcss styles --base styles --ext css --dir app/styles --env production",
    "dev": "concurrently 'yarn run dev:css' 'remix dev'",
    "dev:css": "postcss styles --base styles --ext css --dir app/styles -w",
    "postcss": "postcss",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "test": "echo 'would test'"
  },
  "dependencies": {
    "@codsen/design-tokens": "*",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.5.1",
    "postcss": "^8.4.5",
    "postcss-nested-import": "^0.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.1.0",
    "postcss-reporter": "^7.0.4",
    "postcss-sorting": "^7.0.1",
    "postcss-cli": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
